{

  "exclude": ["./coverage", "./dist", "__tests__", "jest.config.js"],
  "ts-node": {
  "transpileOnly": true,
  "files": true
  },

    "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "node",
      "strict": true,
      "outDir": "./built",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*"],
}